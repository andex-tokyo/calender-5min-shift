{
  "manifest_version": 3,
  "name": "Google Calendar Shift 5min",
  "version": "1.0",
  "description": "Add a button to shift calendar events by 5 minutes",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://calendar.google.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
